<div id="carousel_wrapper"> <!-- start carousel_wrapper -->

  <div id="fwslider"> <!-- start fwslider -->
    <div class="slider_container"> <!-- start slider_container -->

      <div class="slide">
        <%= image_tag '/assets/carousel_01.jpg' %>
        <div class="slide_content">
          <div class="slide_content_wrap">
            <h4 class="title">Project Blue</h4>
            <p class="description">Get involved and help Project Blue keep the ocean clean.</p>
            <a class="readmore" href="#">Learn More</a>
          </div>
        </div>
      </div>

      <div class="slide">
        <%= image_tag '/assets/carousel_02.jpg' %>
        <div class="slide_content">
          <div class="slide_content_wrap">
            <h4 class="title">Stomp Out Hunger</h4>
            <p class="description">Join Stomp Out Hunger in the fight against starvation.</p>
            <a class="readmore" href="#">Learn More</a>
          </div>
        </div>
      </div>

      <div class="slide">
        <%= image_tag '/assets/carousel_03.jpg' %>
        <div class="slide_content">
          <div class="slide_content_wrap">
            <h4 class="title">Clean Air</h4>
            <p class="description">Keep earth green through Clean Air's partnership with Geek Seek.</p>
            <a class="readmore" href="#">Learn More</a>
          </div>
        </div>
      </div>

    </div> <!-- end slider_container -->

    <div class="timers"></div>
    <div class="slidePrev"><span></span></div>
    <div class="slideNext"><span></span></div>

  </div> <!-- end fwslider -->
</div> <!-- end carousel_wrapper -->

